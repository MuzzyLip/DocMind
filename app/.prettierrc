    "tabWidth": 4,
    "printWidth": 100,
    "useTabs": false,
    "semi": true,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "quoteProps": "consistent",
    "overrides": [
        {
            "files": ["*.json", "*.yaml", "*.yml", ".cjs"],
            "options": {
                "tabWidth": 4
            }
        }
    ]
}
